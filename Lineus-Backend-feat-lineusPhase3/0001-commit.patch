+package kr.lineedu.lms.feature.attendance.api;
+
+import java.time.LocalDate;
+import java.util.ArrayList;
+import java.util.List;
+import java.util.Map;
+import java.util.Optional;
+import java.util.Set;
+import java.util.stream.Collectors;
+
+import kr.lineedu.lms.config.constant.UrlUtil;
+import kr.lineedu.lms.feature.attendance.api.dto.*;
+import kr.lineedu.lms.global.feign.SubjectClient;
+import lombok.extern.slf4j.Slf4j;
+import org.springframework.stereotype.Service;
+import org.springframework.transaction.annotation.Transactional;
+
+import kr.lineedu.lms.feature.attendance.domain.Attendance;
+import kr.lineedu.lms.feature.attendance.domain.AttendanceRepository;
+import kr.lineedu.lms.feature.attendanceOnlineSetting.domain.AttendanceOnlineSetting;
+import kr.lineedu.lms.feature.attendanceOnlineSetting.domain.AttendanceOnlineSettingRepository;
+import kr.lineedu.lms.feature.common.api.dto.feign.UserEnrollmentFeignDto;
+import kr.lineedu.lms.feature.course.api.CourseService;
+import kr.lineedu.lms.feature.course.api.dto.feign.ItemInfoFeignDto;
+import kr.lineedu.lms.feature.course.api.dto.feign.ModuleInfoFeignDto;
+import kr.lineedu.lms.feature.panopto.api.PanoptoService;
+import kr.lineedu.lms.feature.panopto.api.dto.feign.FolderFeignDto;
+import kr.lineedu.lms.feature.panopto.api.dto.feign.FolderResultFeigntDto;
+import kr.lineedu.lms.feature.panopto.api.dto.response.VideoDto;
+import kr.lineedu.lms.feature.panopto.api.dto.response.VideoResultFeignDto;
+import kr.lineedu.lms.feature.panopto.api.dto.response.ViewerDto;
+import kr.lineedu.lms.feature.panopto.api.dto.response.ViewerResultFeignDto;
+import kr.lineedu.lms.feature.subject.api.dto.feign.EnrollmentStudentAttendanceListFeignDto;
+import kr.lineedu.lms.feature.user.domain.User;
+import kr.lineedu.lms.feature.user.domain.UserRepository;
+import kr.lineedu.lms.global.enums.AttendanceState;
+import kr.lineedu.lms.global.enums.VideoState;
+import kr.lineedu.lms.global.error.dto.ErrorCode;
+import kr.lineedu.lms.global.error.exception.BusinessException;
+import kr.lineedu.lms.global.feign.AccountClient;
+import kr.lineedu.lms.global.feign.EnrollmentClient;
+import lombok.RequiredArgsConstructor;
+
+@Slf4j
+@Service
+@RequiredArgsConstructor
+public class AttendanceServiceImpl implements AttendanceService {
+
+    private final AttendanceRepository attendanceRepository;
+    private final UserRepository userRepository;
+
+    @Transactional
+    public List<Long> createAttendance(List<AttendanceCreateDto> attendanceCreateDtoList, Long courseId,
+                                       Long moduleNumber, Long contentNumber, LocalDate date) {
+
+        List<Long> attendanceIdList = new ArrayList<>();
+
+        for (AttendanceCreateDto attendanceCreateDto : attendanceCreateDtoList) {
+            checkDuplicateAttendance(courseId, moduleNumber, contentNumber, attendanceCreateDto, date);
+            attendanceIdList.add(attendanceCreateDto.getUserId());
+        }
+
+        return attendanceIdList;
+    }
+
+    @Transactional(readOnly = true)
+    public List<AttendanceListInfoDto> getAttendanceList(Long courseId) {
+        List<AttendanceListInfoDto> infoDtoList = new ArrayList<>();
+
+        List<AttendanceListProjection> attendanceList = attendanceRepository.findBy(courseId);
+
+        Set<Long> userIds = attendanceList.stream()
+                .map(AttendanceListProjection::getUserId)
+                .collect(Collectors.toSet());
+
+        for (Long userId : userIds) {
+            User user = getUserBy(userId.toString());
+
+            // moduleNumber   List 
+            List<ModuleAttendanceSummaryDto> attendanceSummaryDtoList = attendanceList.stream()
+                    .filter(a -> a.getUserId().equals(userId))
+                    .collect(Collectors.groupingBy(
+                            AttendanceListProjection::getModuleNumber,
+                            Collectors.mapping(
+                                    a -> AttendanceSummaryDto.builder()
+                                            .contentNumber(a.getContentNumber())
+                                            .moduleNumber(a.getModuleNumber())
+                                            .date(a.getDate())
+                                            .attendanceState(AttendanceState.toEnum(a.getAttendanceState()))
+                                            .build(),
+                                    Collectors.toList()
+                            )
+                    ))
+                    .entrySet()
+                    .stream()
+                    .map(entry -> ModuleAttendanceSummaryDto.builder()
+                            .moduleNumber(entry.getKey())
+                            .attendanceSummaries(entry.getValue())
+                            .build()
+                    )
+                    .collect(Collectors.toList());
+
+            long attendanceCnt = countAttendance(attendanceList, userId, AttendanceState.ATTENDANCE);
+            long excusedAbsenceCnt = countAttendance(attendanceList, userId, AttendanceState.EXCUSED_ABSENCE);
+            long lateCnt = countAttendance(attendanceList, userId, AttendanceState.LATE);
+            long absenceCnt = countAttendance(attendanceList, userId, AttendanceState.ABSENCE);
+
+            infoDtoList.add(AttendanceListInfoDto.builder()
+                    .name(user.getName())
+                    .userNumber(user.getLoginId())
+                    .subDivision(user.getSubDivision())
+                    .attendanceCnt(attendanceCnt + excusedAbsenceCnt)
+                    .lateCnt(lateCnt)
+                    .absenceCnt(absenceCnt)
+                    .attendanceSummaryDtoList(attendanceSummaryDtoList) // List  
+                    .build());
+        }
+        return infoDtoList;
+    }
+
+    @Transactional(readOnly = true)
+    public List<OnlineAttendanceListInfoDto> getOnlineAttendanceList(Long courseId) {
+        List<OnlineAttendanceListInfoDto> infoDtoList = new ArrayList<>();
+
+        List<AttendanceListProjection> attendanceList = attendanceRepository.findByOnline(courseId);
+
+        Set<Long> userIds = attendanceList.stream()
+                .map(AttendanceListProjection::getUserId)
+                .collect(Collectors.toSet());
+
+        for (Long userId : userIds) {
+            User user = getUserBy(userId.toString());
+
+            // moduleNumber   List 
+            List<ModuleAttendanceSummaryDto> attendanceSummaryDtoList = attendanceList.stream()
+                    .filter(a -> a.getUserId().equals(userId))
+                    .collect(Collectors.groupingBy(
+                            AttendanceListProjection::getModuleNumber,
+                            Collectors.mapping(
+                                    a -> AttendanceSummaryDto.builder()
+                                            .contentNumber(a.getContentNumber())
+                                            .moduleNumber(a.getModuleNumber())
+                                            .date(a.getDate())
+                                            .description(a.getDescription())
+                                            .classType(a.getClassType())
+                                            .attendanceState(AttendanceState.toEnum(a.getAttendanceState()))
+                                            .build(),
+                                    Collectors.toList()
+                            )
+                    ))
+                    .entrySet()
+                    .stream()
+                    .map(entry -> ModuleAttendanceSummaryDto.builder()
+                            .moduleNumber(entry.getKey())
+                            .attendanceSummaries(entry.getValue())
+                            .build()
+                    )
+                    .collect(Collectors.toList());
+
+            long attendanceCnt = countAttendance(attendanceList, userId, AttendanceState.ATTENDANCE);
+            long excusedAbsenceCnt = countAttendance(attendanceList, userId, AttendanceState.EXCUSED_ABSENCE);
+            long lateCnt = countAttendance(attendanceList, userId, AttendanceState.LATE);
+            long absenceCnt = countAttendance(attendanceList, userId, AttendanceState.ABSENCE);
+
+            infoDtoList.add(OnlineAttendanceListInfoDto.builder()
+                    .name(user.getName())
+                    .userNumber(user.getLoginId())
+                    .subDivision(user.getSubDivision())
+                    .attendanceCnt(attendanceCnt + excusedAbsenceCnt)
+                    .lateCnt(lateCnt)
+                    .absenceCnt(absenceCnt)
+                    .attendanceSummaryDtoList(attendanceSummaryDtoList) // List  
+                    .build());
+        }
+        return infoDtoList;
+    }
+
+    private void checkDuplicateAttendance(Long courseId, Long moduleNumber, Long contentNumber,
+                                          AttendanceCreateDto attendanceCreateDto, LocalDate date) {
+
+        Optional<Attendance> existingAttendance = attendanceRepository.findByAttendance(
+                attendanceCreateDto.getUserId(), courseId, moduleNumber, contentNumber);
+
+        Optional<Attendance> existingAttendanceWithDate = attendanceRepository.findByAttendance(
+                attendanceCreateDto.getUserId(), courseId, moduleNumber, contentNumber, date);
+
+        if (existingAttendance.isPresent() && existingAttendanceWithDate.isPresent()) {
+            //    - update 
+            Attendance attendance = existingAttendanceWithDate.get();
+            attendance.setAttendanceState(AttendanceState.toInteger(attendanceCreateDto.getAttendanceState()));
+            attendance.setDate(date);
+            attendanceRepository.save(attendance); // Update
+        } else if (existingAttendance.isEmpty()) {
+            //    - insert 
+            Attendance newAttendance = Attendance.builder()
+                    .userId(attendanceCreateDto.getUserId())
+                    .contentNumber(contentNumber)
+                    .date(date)
+                    .lmsCourseId(courseId)
+                    .moduleNumber(moduleNumber)
+                    .attendanceState(AttendanceState.toInteger(attendanceCreateDto.getAttendanceState()))
+                    .deletedFlag('N')
+                    .build();
+            attendanceRepository.save(newAttendance); // Insert
+        }
+    }
+
+
+    private long countAttendance(List<AttendanceListProjection> attendanceList, Long userId, AttendanceState state) {
+        return attendanceList.stream()
+            .filter(a -> a.getUserId().equals(userId))
+            .filter(a -> AttendanceState.toEnum(a.getAttendanceState()).equals(state))
+            .count();
+    }
+
+    private User getUserBy(String loginId) {
+        return userRepository.findByLoginId(loginId)
+            .orElseThrow(() -> new BusinessException(ErrorCode.NOT_FOUND_USER_EXCEPTION));
+    }
+
+}
