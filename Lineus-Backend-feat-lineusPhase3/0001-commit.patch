+    @Transactional
+    @Override
+    public List<Long> createGrade(List<GradeCreateDto> gradeCreateDtos, Long courseId) {
+        List<Grade> grades = new ArrayList<>();
+
+        for (GradeCreateDto gradeCreateDto : gradeCreateDtos) {
+            List<Grade> existingGrades = gradeRepository.findByCourseIdAndUserNumber(courseId, gradeCreateDto.getUserNumber())
+                    .orElse(new ArrayList<>()); //      
+
+            if (!existingGrades.isEmpty()) {
+                //     
+                for (Grade existingGrade : existingGrades) {
+                    updateExistingGrade(existingGrade, gradeCreateDto);
+                    grades.add(existingGrade);
+                }
+            } else {
+                // AssignmentGroupGradeCreateDtos 
+                List<AssignmentGroupGradeCreateDto> assignmentGroupGrades = gradeCreateDto.getAssignmentGroupGradeCreateDtos();
+
+                for (AssignmentGroupGradeCreateDto assignmentGroupDto : assignmentGroupGrades) {
+                    Grade assignmentGrade = Grade.builder()
+                            .userName(gradeCreateDto.getUserName())
+                            .userNumber(gradeCreateDto.getUserNumber())
+                            .courseId(courseId)
+                            .gradeGroupId(assignmentGroupDto.getGradeGroupId())
+                            .rawScore(assignmentGroupDto.getGradeRawScore())
+                            .weightScore(assignmentGroupDto.getGradeWeightScore())
+                            .totalPoint(gradeCreateDto.getTotalPoint())
+                            .totalGrade(gradeCreateDto.getTotalGrade())
+                            .build();
+                    grades.add(assignmentGrade);
+                }
+            }
+        }
+
+        gradeRepository.saveAll(grades);
+        return grades.stream()
+                .map(Grade::getId)
+                .toList();
+    }
+
+    @Transactional(readOnly = true)
+    @Override
+    public List<GradeInfoDto> getGrades(Long courseId) {
+
+        List<GradeInfoDto> gradeInfoDtos = new ArrayList<>();
+
+        List<Long> list = courseClient.getCourseStudentList(courseId).stream()
+                .filter(user -> user.getLoginId().length() < 30)
+                .map(CourseStudentIdListFeignDto::getId)
+                .sorted(Comparator.comparing(Long::longValue))
+                .toList();
+
+        List<User> users = userRepository.findByLmsUserIdV2(list); //  
+        List<Object[]> results = gradeRepository.getGradesByCourseId(courseId);
+
+        List<AttendanceListProjection> attendanceList = attendanceRepository.findBy(courseId);
+        List<AttendanceListProjection> onlineAttendanceList = attendanceRepository.findByOnline(courseId);
+        Optional<AttendanceSetting> attendanceSetting = attendanceSettingRepository.findByLmsCourseId(courseId);
+        Optional<AttendanceOnlineSetting> attendanceOnlineSetting = attendanceOnlineSettingRepository.findByLmsCourseId(courseId);
+        List<AssignmentGroupGrade> gradeGroupList = assignmentGroupGradeRepository.findByCourseId(courseId);
+
+        Map<Long, List<AttendanceListProjection>> groupedAttendance = attendanceList.stream()
+                .collect(Collectors.groupingBy(AttendanceListProjection::getUserId));
+
+        Map<Long, List<AttendanceListProjection>> groupedOnlineAttendance = onlineAttendanceList.stream()
+                .collect(Collectors.groupingBy(AttendanceListProjection::getUserId));
+
+        // AssignmentGroupGrade       
+        Optional<AssignmentGroupGrade> onsiteGrade = gradeGroupList.stream()
+                .filter(grade -> grade.getName().equals(" "))
+                .findFirst();
+
+        Optional<AssignmentGroupGrade> onlineGrade = gradeGroupList.stream()
+                .filter(grade -> grade.getName().equals(" "))
+                .findFirst();
+
+        Map<String, GradeInfoDto> gradeInfoMap = new HashMap<>();
+        //  DB    
+        if (results.isEmpty()) {
+            for (User user : users) {
+                GradeInfoDto gradeInfoDto = GradeInfoDto.builder()
+                        .userName(user.getName())
+                        .userNumber(user.getLoginId())
+                        .assignmentGroupGradeCreateDtos(new ArrayList<>())
+                        .build();
+
+                //     
+                for (AssignmentGroupGrade gradeGroup : gradeGroupList) {
+                    if (!gradeGroup.getName().equals(" ") && !gradeGroup.getName().equals(" ")) {
+                        gradeInfoDto.getAssignmentGroupGradeCreateDtos().add(AssignmentGroupGradeInfoDto.builder()
+                                .gradeGroupId(gradeGroup.getId())
+                                .gradeGroupName(gradeGroup.getName())
+                                .isOpen(gradeGroup.getIsOpen() == 'Y')
+                                .build());
+                    }
+                }
+                gradeInfoMap.put(user.getLoginId(), gradeInfoDto);
+            }
+        } else {
+            //  DB   
+            for (Object[] result : results) {
+                String userNumber = (String) result[1];
+                User user = users.stream()
+                        .filter(u -> u.getLoginId().equals(userNumber))
+                        .findFirst()
+                        .orElse(null);
+
+                if (user == null) {
+                    continue;
+                }
+
+                Long gradeGroupId = (Long) result[2];
+                String gradeGroupName = (String) result[3];
+                Integer gradeRawScore = (Integer) result[4];
+                Double gradeWeightScore = (Double) result[5];
+                Character isOpen = (Character) result[6];
+                Double totalPoint = (Double) result[7];
+                String totalGrade = (String) result[8];
+
+                GradeInfoDto gradeInfoDto = gradeInfoMap.getOrDefault(userNumber, GradeInfoDto.builder()
+                        .userName(user.getName())
+                        .userNumber(userNumber)
+                        .assignmentGroupGradeCreateDtos(new ArrayList<>())
+                        .totalPoint(totalPoint)
+                        .totalGrade(totalGrade)
+                        .build());
+
+                if (gradeGroupName != null && !(gradeGroupName.equals(" ") || gradeGroupName.equals(" "))) {
+                    gradeInfoDto.getAssignmentGroupGradeCreateDtos().add(AssignmentGroupGradeInfoDto.builder()
+                            .gradeGroupId(gradeGroupId)
+                            .gradeGroupName(gradeGroupName)
+                            .gradeRawScore(gradeRawScore)
+                            .gradeWeightScore(gradeWeightScore)
+                            .isOpen(isOpen == 'Y')
+                            .build());
+                }
+                gradeInfoMap.put(userNumber, gradeInfoDto);
+            }
+        }
+
+        //   
+        if (attendanceSetting.isPresent() && attendanceSetting.get().getUseFlag() == 'Y') {
+            //   Y 
+            groupedAttendance.forEach((userId, userAttendanceList) -> {
+                String userNumber = String.valueOf(userId);
+                User user = users.stream()
+                        .filter(u -> u.getLoginId().equals(userNumber))
+                        .findFirst()
+                        .orElse(null);
+
+                if (user == null) {
+                    return;
+                }
+
+                GradeInfoDto gradeInfoDto = gradeInfoMap.getOrDefault(userNumber, GradeInfoDto.builder()
+                        .userName(user.getName())
+                        .userNumber(userNumber)
+                        .assignmentGroupGradeCreateDtos(new ArrayList<>())
+                        .build());
+
+                int totalScore = onsiteGrade.get().getScore();
+                double weightedScore = 0;
+
+                for (AttendanceListProjection attendance : userAttendanceList) {
+                    gradeInfoDto.setUserName(attendance.getName());
+                    AttendanceState state = AttendanceState.toEnum(attendance.getAttendanceState());
+
+                    if (state == AttendanceState.ABSENCE) {
+                        totalScore += attendanceSetting.get().getAbsencePenaltyPoint();
+                    } else if (state == AttendanceState.LATE) {
+                        totalScore += attendanceSetting.get().getLatePenaltyPoint();
+                    } else {
+                        totalScore += attendanceSetting.get().getMinPoint();
+                    }
+                }
+
+                if (onsiteGrade.isPresent()) {
+                    AssignmentGroupGrade grade = onsiteGrade.get();
+                    if (grade.getScore() != 0) {
+                        weightedScore = grade.getWeight() * ((double) totalScore / grade.getScore());
+                    }
+                    weightedScore = Math.round(weightedScore * 100) / 100.0;
+                }
+
+                gradeInfoDto.getAssignmentGroupGradeCreateDtos().add(AssignmentGroupGradeInfoDto.builder()
+                        .gradeGroupId(onsiteGrade.get().getId())
+                        .gradeGroupName(" ")
+                        .gradeRawScore(totalScore)
+                        .gradeWeightScore(weightedScore)
+                        .isOpen(onsiteGrade.get().getIsOpen() == 'Y' ? true : false)
+                        .build());
+                gradeInfoMap.put(String.valueOf(userId), gradeInfoDto);
+            });
+        }
+
+        if (attendanceOnlineSetting.isPresent() && attendanceOnlineSetting.get().getUseFlag() == 'Y') {
+            //     Y 
+            groupedOnlineAttendance.forEach((userId, userAttendanceList) -> {
+                String userNumber = String.valueOf(userId);
+                User user = users.stream()
+                        .filter(u -> u.getLoginId().equals(userNumber))
+                        .findFirst()
+                        .orElse(null);
+
+                if (user == null) {
+                    return;
+                }
+
+                GradeInfoDto gradeInfoDto = gradeInfoMap.getOrDefault(userNumber, GradeInfoDto.builder()
+                        .userName(user.getName())
+                        .userNumber(userNumber)
+                        .assignmentGroupGradeCreateDtos(new ArrayList<>())
+                        .build());
+
+
+                int totalScore = onlineGrade.get().getScore();
+                double weightedScore = 0;
+
+                for (AttendanceListProjection attendance : userAttendanceList) {
+                    gradeInfoDto.setUserName(attendance.getName());
+                    AttendanceState state = AttendanceState.toEnum(attendance.getAttendanceState());
+
+                    if (state == AttendanceState.ABSENCE) {
+                        totalScore += attendanceOnlineSetting.get().getAbsencePenaltyPoint();
+                    } else if (state == AttendanceState.LATE) {
+                        totalScore += attendanceOnlineSetting.get().getLatePenaltyPoint();
+                    } else {
+                        totalScore += attendanceOnlineSetting.get().getMinPoint();
+                    }
+                }
+
+                if (onlineGrade.isPresent()) {
+                    AssignmentGroupGrade grade = onlineGrade.get();
+                    if (grade.getScore() != 0) {
+                        weightedScore = grade.getWeight() * ((double) totalScore / grade.getScore());
+                    }
+                    weightedScore = Math.round(weightedScore * 100) / 100.0;
+                }
+
+                gradeInfoDto.getAssignmentGroupGradeCreateDtos().add(AssignmentGroupGradeInfoDto.builder()
+                        .gradeGroupId(onlineGrade.get().getId())
+                        .gradeGroupName(" ")
+                        .gradeRawScore(totalScore)
+                        .gradeWeightScore(weightedScore)
+                        .isOpen(onlineGrade.get().getIsOpen() == 'Y' ? true : false)
+                        .build());
+
+                gradeInfoMap.put(String.valueOf(userId), gradeInfoDto);
+            });
+        }
+
+        gradeInfoDtos.addAll(gradeInfoMap.values());
+        return gradeInfoDtos;
+    }
+
+    //    
+    private void updateExistingGrade(Grade existingGrade, GradeCreateDto gradeCreateDto) {
+        existingGrade.setTotalPoint(gradeCreateDto.getTotalPoint());
+        existingGrade.setTotalGrade(gradeCreateDto.getTotalGrade());
+
+        // AssignmentGroupGradeCreateDtos 
+        List<AssignmentGroupGradeCreateDto> assignmentGroupGrades = gradeCreateDto.getAssignmentGroupGradeCreateDtos();
+        for (AssignmentGroupGradeCreateDto assignmentGroupDto : assignmentGroupGrades) {
+            if (existingGrade.getGradeGroupId().equals(assignmentGroupDto.getGradeGroupId())) {
+                existingGrade.setRawScore(assignmentGroupDto.getGradeRawScore());
+                existingGrade.setWeightScore(assignmentGroupDto.getGradeWeightScore());
+            }
+        }
+    }
+}
+
