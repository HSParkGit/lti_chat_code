+package kr.lineedu.lms.feature.attendance.controller;
+
+import java.time.LocalDate;
+import java.util.List;
+
+import kr.lineedu.lms.feature.attendance.api.dto.*;
+import org.springframework.format.annotation.DateTimeFormat;
+import org.springframework.http.ResponseEntity;
+import org.springframework.web.bind.annotation.*;
+
+import kr.lineedu.lms.feature.attendance.api.AttendanceService;
+import kr.lineedu.lms.global.annotation.LmsUser;
+import kr.lineedu.lms.global.annotation.V1RestController;
+import lombok.RequiredArgsConstructor;
+
+@V1RestController
+@RequiredArgsConstructor
+public class AttendanceController {
+
+    private final AttendanceService attendanceService;
+
+    @PostMapping("/attendance/course/{course_id}")
+    public ResponseEntity<List<Long>> createAttendance(
+            @RequestPart("attendanceCreateDtoList") List<AttendanceCreateDto> attendanceCreateDtoList,
+            @RequestParam("moduleNumber") Long moduleNumber, // 
+            @RequestParam("contentNumber") Long contentNumber, // 
+            @RequestParam("date") @DateTimeFormat(iso = DateTimeFormat.ISO.DATE) LocalDate date,
+            @PathVariable("course_id") Long courseId
+    ) {
+        return ResponseEntity.ok(
+                attendanceService.createAttendance(attendanceCreateDtoList, courseId, moduleNumber, contentNumber, date)
+        );
+    }
+
+    //
+    @GetMapping("/attendance/course/{course_id}")
+    public ResponseEntity<List<AttendanceListInfoDto>> getAttendance(@PathVariable("course_id") Long courseId) {
+        return ResponseEntity.ok(attendanceService.getAttendanceList(courseId));
+    }
+
+    //()
+    @GetMapping("/attendance/course/{course_id}/online")
+    public ResponseEntity<List<OnlineAttendanceListInfoDto>> getOnlineAttendance(@PathVariable("course_id") Long courseId) {
+        return ResponseEntity.ok(attendanceService.getOnlineAttendanceList(courseId));
+    }
+}
